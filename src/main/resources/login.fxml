<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.hotelsearch2h.controllers.LoginController">
    <children>
        <TextField fx:id="user" layoutX="268.0" layoutY="119.0"/>
        <TextField fx:id="pswd" layoutX="269.0" layoutY="187.0"/>
        <Label layoutX="181.0" layoutY="123.0" text="Email:"/>
        <Label layoutX="185.0" layoutY="191.0" text="Password:"/>
        <Button fx:id="btnlogin" layoutX="317.0" layoutY="252.0" mnemonicParsing="false" onAction="#loginButtonClicked"
                text="Login"/>
        <Label fx:id="lblRegister" layoutX="485.0" layoutY="332.0" onMouseClicked="#registerButtonClicked"
               text="Register?" textFill="#0a0ac9">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <Label fx:id="msg" layoutX="300.0" layoutY="369.0" text="" textFill="#ef0404"/>
    </children>
</AnchorPane>
